#include<stdio.h>
#include<conio.h>
#define m 4
#define n 4
int main(void)
{
	int i,j,k, imaxA, imaxB;
	float A[m][n],maxA,maxB,C[m][n];
	printf("Vvedite A[4]*[4]\n");
	for (i=0;i<m;i++)
	for (j=0;j<n;j++)
	scanf("%f",&A[i][j]);
	float B[m][n];
	for (i=0;i<m;i++)
	for (j=0;j<n;j++)
	if(i==j)
	
		(B[i][j]=0);
		else
		(B[i][j]=i+j);
	
	printf(" B[4]*[4]\n");
	for (i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		printf("%3f ",B[i][j]);
		printf("\n");
	}
	maxA=A[0][0];
	for (i=0;i<m;i++)
	for (j=0;j<n;j++)
	if(A[i][j]>maxA)
	{
		maxA=A[i][j];
	}
	printf("maxA=%.3f ", maxA);
	maxB=B[0][0];
	for (i=0;i<m;i++)
	for (j=0;j<n;j++)
	if(B[i][j]>maxB)
	{
		maxB=B[i][j];
	}
	printf("maxB=%.3f\n ", maxB);
	if(maxA>maxB)
	{

	for (i=0;i<n;i++)
	for (j=0;j<n;j++)
	{
	
	C[i][j]=0;
	for (k=0;k<n;k++)
	
	C[i][j]+=A[i][k]*B[k][j];
}
}
	else
{	
     for (i=0;i<n;i++)
	for (j=0;j<n;j++)
	{
	C[i][j]=0;
	for (k=0;k<n;k++)
	C[i][j]+=B[i][k]*A[k][j];
}
}
	
		for(i=0;i<n;i++)
		{
		
		for (j=0;j<n;j++)
		printf("%.3f ",C[i][j]);
		printf("\n");
	}
	}
